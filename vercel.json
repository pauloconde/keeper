{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cronpings",
      "schedule": "0 16 * * *"
    }
  ],
  "functions": {
    "app/api/cronpings/route.js": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"]
}